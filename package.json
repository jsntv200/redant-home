{
  "name": "redant-home",
  "version": "1.3.18",
  "description": "Red Ant Website.",
  "main": "assets/javascript/index.js",
  "author": "Jason Taylor <jason@redant.com.au>",
  "license": "UNLICENSED",
  "engines": {
    "node": "16.13.1"
  },
  "scripts": {
    "dev": "concurrently 'yarn dev:esbuild' 'yarn dev:jekyll'",
    "dev:esbuild": "yarn esbuild --watch",
    "dev:jekyll": "bundle exec jekyll serve --livereload",
    "build": "yarn esbuild && yarn jekyll",
    "esbuild": "node esbuild.config.js",
    "jekyll": "bundle exec jekyll build",
    "preview": "yarn esbuild && yarn preview:jekyll",
    "preview:jekyll": "bundle exec jekyll build --drafts --unpublished --future",
    "release": "release-it"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "aos": "^2.2.0",
    "autoprefixer": "^9.6.1",
    "bootstrap": "4.6.1",
    "concurrently": "^4.1.1",
    "esbuild": "^0.14.27",
    "esbuild-sass-plugin": "^2.2.6",
    "jquery": "^3.6.0",
    "macy": "^2.5.0",
    "popper.js": "^1.15.0",
    "prettier": "^2.6.2",
    "release-it": "12.3.3",
    "rimraf": "^2.6.3",
    "slick-carousel": "^1.8.1",
    "stimulus-reveal-controller": "^4.0.0",
    "stimulus-scroll-to": "^4.0.0"
  }
}
